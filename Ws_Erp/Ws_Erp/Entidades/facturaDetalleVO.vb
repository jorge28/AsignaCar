Public Class facturaDetalleVO
    Private fArticulo As String
    Private fcuenta As Integer
    Private fimporte As Double
    Private fiva As Double
    Private fCentroCostos As String
    Private fImpuesto2 As String
    Private fUnidad As String


    Public Property Articulo As String
        Get
            Return fArticulo
        End Get
        Set(value As String)
            fArticulo = value
        End Set
    End Property

    Public Property Cantidad As String
        Get
            Return fcuenta
        End Get
        Set(ByVal Value As String)
            fcuenta = Value
        End Set
    End Property

    Public Property ImporteT As Double
        Get
            Return fimporte
        End Get
        Set(ByVal Value As Double)
            fimporte = Value
        End Set
    End Property

    Public Property IVA As Double
        Get
            Return fiva
        End Get
        Set(ByVal Value As Double)
            fiva = Value
        End Set
    End Property

    Public Property CentroCostos As String
        Get
            Return fCentroCostos
        End Get
        Set(ByVal Value As String)
            fCentroCostos = Value
        End Set
    End Property

    Public Property Impuesto2 As Double
        Get
            Return fImpuesto2
        End Get
        Set(ByVal Value As Double)
            fImpuesto2 = Value
        End Set
    End Property

    Public Property Unidad As String
        Get
            Return fUnidad
        End Get
        Set(ByVal Value As String)
            fUnidad = Value
        End Set
    End Property

End Class
