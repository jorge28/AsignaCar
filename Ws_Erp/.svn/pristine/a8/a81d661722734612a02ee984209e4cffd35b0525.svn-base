Public Class Ventas

    Public nombre As String
    Public apellido As String

    Public lstempleo As List(Of Registro)

    Private Property _lstempleo As List(Of Registro)
        Get
            Return lstempleo
        End Get
        Set(value As List(Of Registro))
            lstempleo = value
        End Set
    End Property

    Private Property _nombre As String
        Get
            Return nombre
        End Get
        Set(value As String)
            nombre = value
        End Set
    End Property

    Private Property _apellido As String
        Get
            Return apellido
        End Get
        Set(value As String)
            apellido = value
        End Set
    End Property
End Class

Public Class Registro

    Public idregistro As String

    Public nombre As String

    Private Property _idregistro As String
        Get
            Return idregistro
        End Get
        Set(value As String)
            idregistro = value
        End Set
    End Property

    Private Property _nombre As String
        Get
            Return nombre
        End Get
        Set(value As String)
            nombre = value
        End Set
    End Property
End Class
