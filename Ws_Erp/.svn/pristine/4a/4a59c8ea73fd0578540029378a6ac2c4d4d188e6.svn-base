Public Class objError

    Private errores As Boolean
    Private mensaje As String
    Private idServicio As String
    Private strProveedor As String
    Private intNumeroVal As Integer
    Private intNumeroError As Integer

    Public Property NumeroError() As Integer
        Get
            Return intNumeroError
        End Get
        Set(ByVal value As Integer)
            intNumeroError = value
        End Set
    End Property

    Public Property NumeroVal() As Integer
        Get
            Return intNumeroVal
        End Get
        Set(ByVal value As Integer)
            intNumeroVal = value
        End Set
    End Property

    Public Property ObjError() As Boolean
        Get
            Return errores
        End Get
        Set(ByVal value As Boolean)
            errores = value
        End Set
    End Property

    Public Property Proveedor() As String
        Get
            Return strProveedor
        End Get
        Set(ByVal value As String)
            strProveedor = value
        End Set
    End Property

    Public Property Id() As String
        Get
            Return IdServicio
        End Get
        Set(ByVal value As String)
            idServicio = value
        End Set
    End Property

    Public Property StrMensaje() As String
        Get
            Return mensaje
        End Get
        Set(ByVal value As String)
            mensaje = value
        End Set
    End Property

End Class
